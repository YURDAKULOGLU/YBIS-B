{
  "name": "ybis-mobile",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace YBISMobile.xcworkspace -scheme YBISMobile -configuration Release",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "clean": "react-native clean-project-auto"
  },
  "dependencies": {
    "react": "18.3.1",
    "react-native": "0.76.8",
    
    "@react-navigation/native": "6.1.18",
    "@react-navigation/stack": "6.4.1", 
    "@react-navigation/bottom-tabs": "6.6.1",
    "react-native-screens": "3.34.0",
    "react-native-safe-area-context": "4.10.9",
    "react-native-gesture-handler": "2.18.1",
    
    "zustand": "5.0.8",
    "axios": "1.12.2",
    "@react-native-async-storage/async-storage": "1.24.0",
    
    "react-native-gifted-chat": "2.8.1",
    "react-native-markdown-display": "7.0.2",
    "react-native-vector-icons": "10.3.0",
    
    "react-hook-form": "7.62.0",
    "zod": "4.1.8",
    
    "nativewind": "4.2.1",
    "react-native-reanimated": "3.15.5",
    
    "i18n-js": "4.5.1",
    "date-fns": "4.1.0",
    "nanoid": "5.1.5",
    
    "@ybis/core": "workspace:*",
    "@ybis/api-client": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "7.25.9",
    "@babel/preset-env": "7.25.9",
    "@babel/runtime": "7.25.9",
    
    "@react-native/babel-preset": "0.76.8",
    "@react-native/eslint-config": "0.76.8",
    "@react-native/metro-config": "0.76.8",
    "@react-native/typescript-config": "0.76.8",
    
    "@types/react": "18.3.24",
    "@types/react-test-renderer": "18.3.1",
    
    "eslint": "9.35.0",
    "prettier": "3.6.2",
    "typescript": "5.9.2",
    
    "jest": "30.1.3",
    "babel-jest": "30.1.2",
    "react-test-renderer": "18.3.1",
    "@testing-library/react-native": "12.8.1",
    
    "tailwindcss": "4.1.13",
    "postcss": "8.5.6",
    
    "@tsconfig/react-native": "3.0.6",
    "babel-plugin-module-resolver": "^5.0.2"
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": ["@testing-library/jest-native/extend-expect"]
  },
  "engines": {
    "node": ">=20.19.4",
    "npm": ">=10.0.0"
  }
}