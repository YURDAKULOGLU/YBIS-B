{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "core",
  "projectType": "library",
  "sourceRoot": "packages/core/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run build",
        "cwd": "packages/core"
      },
      "outputs": ["{projectRoot}/dist"]
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm test",
        "cwd": "packages/core"
      }
    }
  }
}
